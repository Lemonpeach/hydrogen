    "boilerplate",
    "feathers"
  ],
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/Lemonpeach/react-redux-yolo/issues"
  },
  "homepage": "https://github.com/Lemonpeach/react-redux-yolo#readme",
  "peerDependencies": {
